class A
{
	public void methodA()
	{
		System.out.println("A-class");
	}
}


public class SingleLevel extends A {
	
	public void methodB() 
	{
		System.out.println("B-class");
	}
	
	public static void main(String args[])
	{
		//A a = new A();
		SingleLevel b = new SingleLevel();
		b.methodA();
		b.methodB();
	}
}
