
public class BankAcc {

	private int accNo;
	private String accNm;
	private float accBal;
	
	
	public int getAccNo() {
		return accNo;
	}
	
	public String getAccNm() {
		return accNm;
	}
	public void setAccNum(String accNum) {
		this.accNm = accNum;
	}
	
	public float getAccBal() {
		return accBal;
	}
	
	
	BankAcc(int accNo, String accNm, float accBal)
	{
		this.accNo = accNo;
		this.accNm = accNm;
		this.accBal = accBal;
	}
	
	public void withdraw(float accBal, float amount)
	{
		accBal = accBal - amount;
	}
	
	public void deposit(float accBal, float amount)
	{
		accBal = accBal + amount;
	}
	
	/*@Override
	public String toString()
	{
		return 
	}*/
}
